{
  "name": "modern_consumer",
  "version": "0.1.0",
  "scripts": {
    "reset": "npx rimraf node_modules ./**/node_modules",
    "clean": "rimraf node_modules/.cache .modern dist",
    "dev": "modern dev",
    "dev:clean": "pnpm run clean && pnpm run dev",
    "build": "modern build",
    "start": "modern start",
    "serve": "modern serve",
    "new": "modern new",
    "lint": "biome check",
    "prepare": "simple-git-hooks",
    "upgrade": "modern upgrade"
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@modern-js/plugin-tailwindcss": "2.68.7",
    "@modern-js/runtime": "2.68.7",
    "@module-federation/bridge-react": "0.21.6",
    "@module-federation/retry-plugin": "0.21.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@modern-js/app-tools": "2.68.7",
    "@modern-js/tsconfig": "2.68.7",
    "@module-federation/modern-js": "0.21.6",
    "@types/jest": "~29.2.4",
    "@types/node": "~18.11.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "~18.3.1",
    "lint-staged": "~13.1.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "~4.1.18",
    "typescript": "~5.0.4"
  }
}
